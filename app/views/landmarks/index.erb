<h2>All Historical Landmarks</h2>

<% @landmarks.each do |landmark| %>
  <li> <a href="/landmarks/<%= landmark.id %>"> <%= landmark.name %> </a></li>
  <li align="center"><%= landmark.year_completed %></li>
<% end %>
<a class="button" href="landmarks/new">Create New Landmark</a>
